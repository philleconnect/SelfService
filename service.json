{
    "containers":[
        {
            "name":"selfservice-redis",
            "hostname":"selfservice-redis",
            "prebuilt":{
                "name":"redis",
                "version":"latest"
            },
            "ports":[],
            "networks":[
                {
                    "name":"selfservice-redis-net"
                }
            ],
            "volumes":[],
            "environment":[]
        },
        {
            "name":"selfservice",
            "hostname":"selfservice",
            "url":"https://github.com/philleconnect/SelfService/releases/download/1.2.7/selfservice.tar.gz",
            "ports":[
                {
                    "external":8001,
                    "internal":80
                },
                {
                    "external":450,
                    "internal":443
                }
            ],
            "networks":[
                {
                    "name":"schoolconnect"
                },
                {
                    "name":"mysql-net"
                },
                {
                    "name":"selfservice-redis-net"
                }
            ],
            "volumes":[
                {
                    "name":"selfservice-redis-data",
                    "mountpoint":"/data"
                }
            ],
            "environment":[
                "MYSQL_PASSWORD",
                "MYSQL_USER",
                "MYSQL_DATABASE",
                "SMTP_SERVER",
                "SMTP_PORT",
                "SMTP_LOGIN",
                "SMTP_PASSWORD",
                "SMTP_SENDER",
                "EXTERNAL_DOMAIN",
                "EMAIL_RESET_ENABLED"
            ]
        }
    ],
    "volumes":[
        {
            "name":"selfservice-redis-data"
        }
    ],
    "networks":[
        {
            "name":"selfservice-redis-net",
            "internal":true
        }
    ],
    "environment":[
        {
            "name":"SMTP_SERVER",
            "description":"URL zum SMTP Server.",
            "mutable":true
        },
        {
            "name":"SMTP_PORT",
            "description":"Zu verwendender Port auf dem SMTP Server.",
            "mutable":true,
            "value":"578"
        },
        {
            "name":"SMTP_LOGIN",
            "description":"Loginname f端r den SMTP Server.",
            "mutable":true
        },
        {
            "name":"SMTP_PASSWORD",
            "description":"Passwort f端r den SMTP Server.",
            "mutable":true
        },
        {
            "name":"SMTP_SENDER",
            "description":"Absendeadresse f端r den SMTP Server.",
            "mutable":true
        },
        {
            "name":"EXTERNAL_DOMAIN",
            "description":"Die externe Domain, unter der PhilleConnect SelfService erreichbar ist. Beispiel: bei https://selfservice.beispiel.de/ muss hier 'selfservice.beispiel.de' eingetragen werden.",
            "mutable":true
        },
        {
            "name":"EMAIL_RESET_ENABLED",
            "description":"Schalter f端r E-Mail Resetfunktion. Wird mittels 'true' aktiviert.",
            "mutable":true
        }
    ]
}
